{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {},
    "overrides": [
        {
            "files": [
                "*.js"
            ],
            "rules": {
                "no-underscore-dangle": "off",
                "import/no-extraneous-dependencies": "off",
                "no-restricted-globals": "off",
                "no-console": "off"
            }
        }
    ]
}